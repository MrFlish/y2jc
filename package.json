  "description": "Compiles yaml to json files. Useful when you want to take advantage of json intellisence in your project, but don't want to write json format in the first place.",
  "scripts": {
    "test": "jest",
    "test:w": "jest --watchAll",
    "tsc": "tsc --watch"
  },
  "main": "dist/bin/index.js",
